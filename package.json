{
  "name": "@kupibilet/ui",
  "version": "2.1.0",
  "description": "UI kit for kupibilet web app",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint:js --silent && npm run lint:css --silent",
    "test": "npm run jest --silent",
    "start": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook",
    "lint:css": "echo \"TODO: Fix stylelint. kupibilet-frontend/ui#1\" >&2 && exit 0; stylelint ./src/components/**/*.js",
    "lint:js": "eslint ./src/**/*.js",
    "jest": "jest",
    "flow": "flow",
    "postinstall": "move-bin || true",
    "build": "NODE_ENV=build babel src/ -d ./ --ignore stories",
    "prepublish": "in-publish && npm run build --silent || not-in-publish",
    "prepush": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.amazon/frontend/ui"
  },
  "author": "Kupibilet.ru Frontend Team",
  "license": "MIT",
  "devDependencies": {
    "@kupibilet/icons": "~3.3.0",
    "@storybook/addon-centered": "^3.0.2",
    "@storybook/addon-info": "^3.0.2",
    "@storybook/addon-knobs": "^3.0.2",
    "@storybook/addon-storyshots": "^3.0.2",
    "@storybook/addons": "^3.0.2",
    "@storybook/react": "^3.0.2",
    "babel-cli": "~6.24.0",
    "babel-jest": "~20.0.3",
    "babel-plugin-flow-react-proptypes": "^3.2.0",
    "babel-plugin-transform-react-jsx-source": "~6.22.0",
    "babel-polyfill": "~6.23.0",
    "babel-preset-env": "~1.5.0",
    "babel-preset-stage-2": "~6.24.1",
    "classnames": "~2.2.5",
    "eslint-config-kupibilet": "^8.2.3",
    "flow-bin": "^0.47.0",
    "husky": "~0.13.2",
    "in-publish": "^2.0.0",
    "jest": "~20.0.0",
    "lint-staged": "^3.5.1",
    "moment": "~2.18.1",
    "move-binaries-to-bin": "~1.0.1",
    "prop-types": "^15.5.8",
    "raw-loader": "~0.5.1",
    "react": "~15.5.4",
    "react-addons-shallow-compare": "~15.5.2",
    "react-dom": "~15.5.4",
    "react-test-renderer": "^15.5.4",
    "styled-components": "^2.0.1",
    "stylelint-config-kupibilet": "^3.1.2",
    "stylelint-processor-styled-components": "~0.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "@kupibilet/react-dates": "~8.2.0",
    "lodash": "^4.17.4",
    "polished": "^1.0.4",
    "rc-collapse": "~1.7.0",
    "rc-tabs": "^7.5.0",
    "react-autosuggest": "^9.0.0",
    "react-onclickoutside": "^5.11.1",
    "section-iterator": "^2.0.0"
  },
  "peerDependencies": {
    "@kupibilet/icons": "^3.4.3",
    "classnames": "^2.2.5",
    "moment": "^2.18.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.0",
    "react-addons-shallow-compare": "^15.5.2",
    "react-dom": "^15.5.4",
    "styled-components": "^2.0.1"
  }
}
