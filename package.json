{
  "name": "@kupibilet/ui",
  "version": "3.6.17-0",
  "description": "UI kit for kupibilet web app",
  "scripts": {
    "pretest": "yarn lint:js && yarn lint:css",
    "test": "yarn jest",
    "prestart": "scripts/check-engines.js",
    "start": "BABEL_ENV=default start-storybook -p 9001 -c .storybook",
    "build-storybook": "BABEL_ENV=default build-storybook",
    "lint:css": "echo \"TODO: Fix stylelint. kupibilet-frontend/ui#1\" >&2 && exit 0; stylelint ./src/components/**/*.js",
    "lint:js": "BABEL_ENV=default eslint \"./src/**/*.js\"",
    "jest": "BABEL_ENV=default jest --bail --no-cache",
    "flow": "flow",
    "prebuild": "scripts/check-engines.js",
    "build": "BABEL_ENV=esmodules NODE_ENV=production babel src/ -d ./ --ignore stories,test",
    "watch": "yarn build --watch",
    "//": "---        Git and NPM hooks section        ---",
    "postinstall": "move-bin; true",
    "prepublishOnly": "yarn build",
    "precommit": "BABEL_ENV=default lint-staged",
    "prepush": "yarn run test",
    "versionize": "versionize"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "yarn lint:js",
      "yarn lint:css",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=0.27.5"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.amazon/frontend/ui"
  },
  "author": "Kupibilet.ru Frontend Team",
  "license": "MIT",
  "devDependencies": {
    "@kupibilet/icons": "~5.1.0",
    "@kupibilet/moment": "^0.0.3",
    "@storybook/addon-centered": "^3.2.12",
    "@storybook/addon-info": "^3.2.12",
    "@storybook/addon-knobs": "^3.2.12",
    "@storybook/addon-options": "^3.2.12",
    "@storybook/addon-storyshots": "^3.2.12",
    "@storybook/addons": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "babel-cli": "~6.26.0",
    "babel-jest": "^22.0.0",
    "babel-plugin-flow-react-proptypes": "^23.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-react-jsx-source": "~6.22.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-stage-2": "~6.24.1",
    "chalk": "^2.1.0",
    "classnames": "~2.2.5",
    "eslint-config-kupibilet": "^9.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.70.0",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "jest": "^22.0.0",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.0.4",
    "lodash": "^4.17.4",
    "mkp": "^1.0.1",
    "move-binaries-to-bin": "~1.0.1",
    "polished": "^1.3.0",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.0",
    "raw-loader": "~0.5.1",
    "react": "^16.3.1",
    "react-addons-shallow-compare": "~15.5.2",
    "react-dom": "^16.3.1",
    "react-onclickoutside": "^6.6.0",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.0.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-logger": "^3.0.6",
    "semver": "^5.4.1",
    "styled-components": "^3.2.5",
    "versionize": "^1.0.6",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "@kupibilet/react-dates": "^12.3.0",
    "@kupibilet/react-media": "^1.5.1",
    "rc-collapse": "^1.9.0",
    "rc-tabs": "^9.1.10",
    "react-autosuggest": "^9.0.0",
    "react-portal": "^4.1.4",
    "rheostat": "^2.1.3",
    "section-iterator": "^2.0.0"
  },
  "peerDependencies": {
    "@kupibilet/icons": "~5.1.0",
    "@kupibilet/moment": "^0.0.3",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "polished": "^1.3.0",
    "prop-types": "^15.6.0",
    "react": "^15.5.0 || ^16.3.0",
    "react-addons-shallow-compare": "~15.5.2",
    "react-dom": "^15.5.0 || ^16.3.0",
    "react-onclickoutside": "^6.4.0",
    "styled-components": "^3.2.5"
  }
}
