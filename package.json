{
  "name": "@kupibilet/ui",
  "version": "1.0.0",
  "description": "UI kit for kupibilet web app",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm run lint:js --silent && npm run lint:css --silent",
    "test": "npm run jest --silent",
    "start": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook",
    "lint:css": "stylelint ./components/**/*.js",
    "lint:js": "eslint ./components/**/*.js",
    "jest": "jest",
    "postinstall": "move-bin",
    "build": "babel components/ -d lib/ --ignore stories",
    "prepublish": "npm run build --silent"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.amazon/frontend/ui"
  },
  "author": "Kupibilet.ru Frontend Team",
  "license": "MIT",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@kadira/react-storybook-decorator-centered": "^1.0.0",
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addon-options": "^1.0.1",
    "babel-cli": "^6.24.0",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.16.1",
    "eslint-config-kupibilet": "^8.1.1",
    "jest": "^18.1.0",
    "move-binaries-to-bin": "^1.0.1",
    "raw-loader": "^0.5.1",
    "stylelint": "^7.9.0",
    "stylelint-config-kupibilet": "^3.0.0",
    "stylelint-processor-styled-components": "0.0.4"
  },
  "dependencies": {
    "@kupibilet/icons": "^3.2.3",
    "@kupibilet/react-dates": "^7.0.1",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.4.2",
    "storyshots": "^3.2.1",
    "styled-components": "^1.4.2"
  }
}
