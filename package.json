{
  "name": "@kupibilet/ui",
  "version": "1.1.3",
  "description": "UI kit for kupibilet web app",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm run lint:js --silent && npm run lint:css --silent",
    "test": "npm run jest --silent",
    "start": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook",
    "lint:css": "echo \"TODO: Fix stylelint. kupibilet-frontend/ui#1\" && exit 0; stylelint ./src/components/**/*.js",
    "lint:js": "eslint ./src/components/**/*.js",
    "jest": "jest",
    "postinstall": "move-bin || true",
    "build": "NODE_ENV=build babel src/ -d lib/ --ignore stories",
    "prepublish": "npm run build --silent"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.amazon/frontend/ui"
  },
  "author": "Kupibilet.ru Frontend Team",
  "license": "MIT",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@kadira/react-storybook-decorator-centered": "^1.0.0",
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addon-options": "^1.0.1",
    "babel-cli": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.16.1",
    "eslint-config-kupibilet": "^8.1.1",
    "husky": "^0.13.2",
    "jest": "^19.0.0",
    "moment": "^2.17.1",
    "move-binaries-to-bin": "^1.0.1",
    "raw-loader": "^0.5.1",
    "stylelint": "^7.9.0",
    "stylelint-config-kupibilet": "^3.0.0",
    "stylelint-processor-styled-components": "0.0.4"
  },
  "dependencies": {
    "@kupibilet/icons": "^3.2.4",
    "@kupibilet/react-dates": "^8.2.0",
    "antd": "^2.8.2",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "rc-collapse": "^1.6.12",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.4.2",
    "storyshots": "^3.2.2",
    "styled-components": "^1.4.2"
  }
