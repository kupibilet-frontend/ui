{
  "name": "@kupibilet/ui",
  "version": "3.6.3",
  "description": "UI kit for kupibilet web app",
  "scripts": {
    "pretest": "yarn lint:js && yarn lint:css",
    "test": "yarn jest",
    "prestart": "scripts/check-engines.js",
    "start": "BABEL_ENV=default start-storybook -p 9001 -c .storybook",
    "build-storybook": "BABEL_ENV=default build-storybook",
    "lint:css": "echo \"TODO: Fix stylelint. kupibilet-frontend/ui#1\" >&2 && exit 0; stylelint ./src/components/**/*.js",
    "lint:js": "BABEL_ENV=default eslint \"./src/**/*.js\"",
    "jest": "BABEL_ENV=default jest --bail --no-cache",
    "flow": "flow",
    "prebuild": "scripts/check-engines.js",
    "build": "BABEL_ENV=esmodules NODE_ENV=production babel src/ -d ./ --ignore stories,test",
    "can-publish": "scripts/is-able-to-publish.js",
    "fetch-git-tags": "git tag | xargs git tag -d &> /dev/null && git fetch origin --tags &> /dev/null",
    "predrop-pre-tags": "git tag | grep - | xargs git push --no-verify --delete origin &> /dev/null",
    "drop-pre-tags": "git tag | grep - | xargs git tag -d &> /dev/null",
    "//": "---        Git and NPM hooks section        ---",
    "postinstall": "move-bin; true",
    "preversion": "yarn fetch-git-tags && yarn drop-pre-tags",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "yarn build",
    "precommit": "BABEL_ENV=default lint-staged",
    "prepush": "yarn run test",
    "postmerge": "yarn fetch-git-tags",
    "update": "yarn drop-pre-tags"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=0.27.5"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.amazon/frontend/ui"
  },
  "author": "Kupibilet.ru Frontend Team",
  "license": "MIT",
  "devDependencies": {
    "@kupibilet/icons": "~5.0.0",
    "@kupibilet/moment": "^0.0.2",
    "@storybook/addon-centered": "^3.2.12",
    "@storybook/addon-info": "^3.2.12",
    "@storybook/addon-knobs": "^3.2.12",
    "@storybook/addon-options": "^3.2.12",
    "@storybook/addon-storyshots": "^3.2.12",
    "@storybook/addons": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "babel-cli": "~6.26.0",
    "babel-jest": "^22.0.0",
    "babel-plugin-flow-react-proptypes": "^13.0.0",
    "babel-plugin-module-resolver": "2.6.2",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-react-jsx-source": "~6.22.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-stage-2": "~6.24.1",
    "chalk": "^2.1.0",
    "classnames": "~2.2.5",
    "eslint-config-kupibilet": "^9.3.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "flow-bin": "^0.63.0",
    "glob": "^7.1.2",
    "husky": "~0.13.2",
    "jest": "^22.0.0",
    "jest-styled-components": "^4.6.0",
    "lint-staged": "^5.0.0",
    "lodash": "^4.17.4",
    "mkp": "^1.0.1",
    "move-binaries-to-bin": "~1.0.1",
    "polished": "^1.3.0",
    "prop-types": "^15.6.0",
    "raw-loader": "~0.5.1",
    "react": "^16.0.0",
    "react-addons-shallow-compare": "~15.5.2",
    "react-dom": "^16.0.0",
    "react-onclickoutside": "^6.6.0",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.0.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-logger": "^3.0.6",
    "semver": "^5.4.1",
    "styled-components": "^2.1.2",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "@kupibilet/react-dates": "^12.3.0",
    "@kupibilet/react-media": "^1.5.1",
    "rc-collapse": "~1.7.0",
    "rc-tabs": "^9.1.10",
    "react-autosuggest": "^9.0.0",
    "react-portal": "^4.0.0-beta.2",
    "rheostat": "^2.1.3",
    "section-iterator": "^2.0.0"
  },
  "peerDependencies": {
    "@kupibilet/icons": "^5.0.0",
    "@kupibilet/moment": "^0.0.2",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "polished": "^1.3.0",
    "prop-types": "^15.6.0",
    "react": "^15.5.0 || ^16.0.0",
    "react-addons-shallow-compare": "~15.5.2",
    "react-dom": "^15.5.0 || ^16.0.0",
    "react-onclickoutside": "^6.4.0",
    "styled-components": "^2.1.2"
  }
}
