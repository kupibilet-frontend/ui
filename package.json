{
  "name": "@kupibilet/ui",
  "version": "3.0.0-0",
  "description": "UI kit for kupibilet web app",
  "scripts": {
    "pretest": "yarn run lint:js --silent && yarn run lint:css --silent",
    "test": "yarn run jest --silent",
    "start": "BABEL_ENV=default start-storybook -p 9001 -c .storybook",
    "build-storybook": "BABEL_ENV=default build-storybook",
    "lint:css": "echo \"TODO: Fix stylelint. kupibilet-frontend/ui#1\" >&2 && exit 0; stylelint ./src/components/**/*.js",
    "lint:js": "BABEL_ENV=default eslint \"./src/**/*.js\"",
    "jest": "BABEL_ENV=default jest --bail --no-cache",
    "flow": "flow",
    "postinstall": "move-bin || true",
    "build": "BABEL_ENV=esmodules NODE_ENV=production babel src/ -d ./ --ignore stories,test",
    "prepublish": "in-publish && yarn run build --silent || not-in-publish",
    "precommit": "BABEL_ENV=default lint-staged",
    "prepush": "yarn run test --silent",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.amazon/frontend/ui"
  },
  "author": "Kupibilet.ru Frontend Team",
  "license": "MIT",
  "devDependencies": {
    "@kupibilet/icons": "~4.0.0",
    "@storybook/addon-centered": "^3.0.2",
    "@storybook/addon-info": "^3.0.2",
    "@storybook/addon-knobs": "^3.0.2",
    "@storybook/addon-storyshots": "^3.0.2",
    "@storybook/addons": "^3.0.2",
    "@storybook/react": "^3.0.2",
    "babel-cli": "~6.24.0",
    "babel-jest": "~20.0.3",
    "babel-plugin-flow-react-proptypes": "^4.0.0",
    "babel-plugin-module-resolver": "2.6.2",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-react-jsx-source": "~6.22.0",
    "babel-polyfill": "~6.23.0",
    "babel-preset-env": "~1.5.0",
    "babel-preset-stage-2": "~6.24.1",
    "classnames": "~2.2.5",
    "eslint-config-kupibilet": "^9.2.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "flow-bin": "^0.52.0",
    "glob": "^7.1.2",
    "husky": "~0.13.2",
    "in-publish": "^2.0.0",
    "jest": "~20.0.0",
    "jest-styled-components": "^4.3.0",
    "lint-staged": "^3.5.1",
    "lodash": "^4.17.4",
    "mkp": "^1.0.1",
    "moment": "~2.18.1",
    "move-binaries-to-bin": "~1.0.1",
    "polished": "^1.3.0",
    "prop-types": "^15.5.8",
    "raw-loader": "~0.5.1",
    "react": "~15.5.4",
    "react-addons-shallow-compare": "~15.5.2",
    "react-dom": "~15.5.4",
    "react-onclickoutside": "^6.4.0",
    "react-test-renderer": "^15.5.4",
    "styled-components": "^2.1.2",
    "stylelint-config-kupibilet": "^3.1.2",
    "stylelint-processor-styled-components": "~0.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "@kupibilet/react-dates": "~8.2.0",
    "@kupibilet/react-media": "^1.5.1",
    "rc-collapse": "~1.7.0",
    "rc-tabs": "^8.1.1",
    "react-autosuggest": "^9.0.0",
    "section-iterator": "^2.0.0"
  },
  "peerDependencies": {
    "@kupibilet/icons": "^4.0.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.18.0",
    "polished": "^1.3.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.0",
    "react-addons-shallow-compare": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-onclickoutside": "^6.4.0",
    "styled-components": "^2.1.2"
  }
}
