{
  "env": {
    "default": {
      "plugins": [
        ["flow-react-proptypes"],
        ["@babel/plugin-proposal-class-properties"],
        ["@babel/plugin-proposal-export-default-from"],
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "blocks": "./src/blocks",
            "components": "./src/components",
            "utils": "./src/utils",
            "storybook": "./storybook"
          }
        }],
        ["styled-components", {
          "minify": false,
        }]
      ],
      "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-flow"]
    },
    "esmodules": {
      "plugins": [
        ["flow-react-proptypes"],
        ["@babel/plugin-proposal-class-properties"],
        ["@babel/plugin-proposal-export-default-from"],
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "blocks": "./src/blocks",
            "components": "./src/components",
            "utils": "./src/utils"
          }
        }],
        ["styled-components", {
          "ssr": true
        }]
      ],
      "presets": [
        ["@babel/preset-env", {"modules": false}],
        "@babel/preset-react",
        "@babel/preset-flow"
      ]
    }
  }
}
