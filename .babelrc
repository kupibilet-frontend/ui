{
  "env": {
    "development": {
      "presets": ["env", "react", "stage-2"],
      "plugins": ["flow-react-proptypes", "styled-components"]
    },
    "production": {
      "presets": ["env", "react", "stage-2"],
      "plugins": ["flow-react-proptypes"]
    },
    "test": {
      "presets": ["env", "react", "stage-2"],
      "plugins": ["flow-react-proptypes"]
    },
    "build": {
      "presets": [["env", {"modules": false}], "react", "stage-2"],
      "plugins": ["flow-react-proptypes"]
    }
  }
}
