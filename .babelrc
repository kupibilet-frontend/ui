{
  "env": {
    "default": {
      "plugins": [
        ["flow-react-proptypes"],
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "blocks": "./src/blocks",
            "components": "./src/components",
            "utils": "./src/utils",
            "storybook": "./storybook"
          }
        }],
        ["styled-components", {
          "minify": false,
        }]
      ],
      "presets": ["env", "react", "stage-2"]
    },
    "esmodules": {
      "plugins": [
        ["flow-react-proptypes"],
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "blocks": "./src/blocks",
            "components": "./src/components",
            "utils": "./src/utils"
          }
        }],
        ["styled-components", {
          "ssr": true
        }]
      ],
      "presets": [["env", {"modules": false}], "react", "stage-2"]
    }
  }
}
